@model List<AppUser>

<div class="card">
    <div class="card-body">
        <div class="card-title d-flex align-items-start justify-content-between">
            <div class="avatar flex-shrink-0">
                <img src="~/dashboard/assets/img/icons/unicons/paypal.png" 
                     alt="Credit Card"
                     class="rounded" />
            </div>
            <div class="dropdown">
                <button class="btn p-0"
                        type="button"
                        id="cardOpt6"
                        data-bs-toggle="dropdown"
                        aria-haspopup="true"
                        aria-expanded="false">
                    <i class="bx bx-dots-vertical-rounded"></i>
                </button>
                <div class="dropdown-menu dropdown-menu-end" aria-labelledby="cardOpt6">
                    <a class="dropdown-item" href="javascript:void(0);">Detay Gör</a>
                    <a class="dropdown-item" href="javascript:void(0);">Sil</a>
                </div>
            </div>
        </div>
        <span>Yönetici Sayısı</span>
        <h3 class="card-title text-nowrap mb-1">@Model.Count()</h3>
        <small class="text-success fw-semibold"><i class="bx bx-up-arrow-alt"></i> +28.42%</small>
    </div>
</div>
                   




                  